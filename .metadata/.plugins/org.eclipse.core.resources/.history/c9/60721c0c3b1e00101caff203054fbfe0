package com.fjpp.expos.screens;

import javax.swing.*;

public class MainScreen {
	
	public JFrame frame;
	JTextField welcome;
	
	public MainScreen() {
		
		frame = new JFrame();
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame.setSize(500, 500);
		frame.setLayout(null);
		frame.setVisible(true);
		
		welcome = new JTextField();
	}
}
